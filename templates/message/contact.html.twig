{% extends 'base.html.twig' %}
{% block title %}Contact {% endblock %}
{% block body %}
	
		
		<!-- contact section start -->
		<div class="contact_section layout_padding">
			<div class="container-fluid">
				<h1 class="what_taital">Contactez-nous</h1>
				<p class="amet_text"> Veuillez renseigner votre Nom, Numéro de téléphone ainsi que votre Adresse mail avant de saisir votre message
				</p>
				<div class="contact_section2">
					<div class="row">
						<div class="col-md-6 padding_left_0">
							<div class="mail_section">
								<form method="post">
									<input type="" class="mail_text_1" placeholder="Nom" name="Nom">
									<input type="" class="mail_text_1" placeholder="Telephone" name="Telephone">
									<input type="" class="mail_text_1" placeholder="Email" name="Email">
									<textarea class="massage_text" placeholder="Contenu" rows="5" id="comment" name="Contenu"></textarea>
									<div class="send_bt">
										<input type="submit"><a href="#">Envoyer</a></input>
									</div>
								</form>
							</div>
						</div>
						<div class="col-md-6 padding_0">
							<div class="map-responsive">
								<iframe src="https://www.google.com/maps/embed/v1/place?key=AIzaSyA0s1a7phLN0iaD6-UE7m4qP-z21pH0eSc&amp;q=Eiffel+Tower+Paris+France" width="600" height="400" frameborder="0" style="border:0; width: 100%;" allowfullscreen=""></iframe>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- contact section end -->
		
	
{% endblock %}
